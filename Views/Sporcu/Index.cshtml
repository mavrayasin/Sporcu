            <th>
                @Html.DisplayNameFor(model => model.DogumTarihi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cinsiyet)
            </th>
       
            <th>
                @Html.DisplayNameFor(model => model.IlId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IlceId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LisansBaslangic)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LisansBitis)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AktifMi)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TcKimlikNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AdSoyad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DogumTarihi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cinsiyet)
