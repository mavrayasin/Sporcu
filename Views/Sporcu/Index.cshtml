                @Html.DisplayFor(modelItem => item.IlId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IlceId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LisansBaslangic)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LisansBitis)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AktifMi)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
